{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "settings":{
        "import/resolver":{
            "node":{
                "extensions": [".js",".ts"]
            }
        }
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "arrow-parens" : "off",
        "comma-dangle" : "off",
        "spaced-comment" : "off"
    }
}
